{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "The AWS CloudFormation template for this Serverless application",
  "Resources": {
    "ServerlessDeploymentBucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketEncryption": {
          "ServerSideEncryptionConfiguration": [
            {
              "ServerSideEncryptionByDefault": {
                "SSEAlgorithm": "AES256"
              }
            }
          ]
        }
      }
    },
    "ServerlessDeploymentBucketPolicy": {
      "Type": "AWS::S3::BucketPolicy",
      "Properties": {
        "Bucket": {
          "Ref": "ServerlessDeploymentBucket"
        },
        "PolicyDocument": {
          "Statement": [
            {
              "Action": "s3:*",
              "Effect": "Deny",
              "Principal": "*",
              "Resource": [
                {
                  "Fn::Join": [
                    "",
                    [
                      "arn:",
                      {
                        "Ref": "AWS::Partition"
                      },
                      ":s3:::",
                      {
                        "Ref": "ServerlessDeploymentBucket"
                      },
                      "/*"
                    ]
                  ]
                },
                {
                  "Fn::Join": [
                    "",
                    [
                      "arn:",
                      {
                        "Ref": "AWS::Partition"
                      },
                      ":s3:::",
                      {
                        "Ref": "ServerlessDeploymentBucket"
                      }
                    ]
                  ]
                }
              ],
              "Condition": {
                "Bool": {
                  "aws:SecureTransport": false
                }
              }
            }
          ]
        }
      }
    },
    "ExtractUnderscoreloadLogGroup": {
      "Type": "AWS::Logs::LogGroup",
      "Properties": {
        "LogGroupName": "/aws/lambda/flights-app-dev-extract_load"
      }
    },
    "TransformUnderscoreloadLogGroup": {
      "Type": "AWS::Logs::LogGroup",
      "Properties": {
        "LogGroupName": "/aws/lambda/flights-app-dev-transform_load"
      }
    },
    "IamRoleLambdaExecution": {
      "Type": "AWS::IAM::Role",
      "Properties": {
        "AssumeRolePolicyDocument": {
          "Version": "2012-10-17",
          "Statement": [
            {
              "Effect": "Allow",
              "Principal": {
                "Service": [
                  "lambda.amazonaws.com"
                ]
              },
              "Action": [
                "sts:AssumeRole"
              ]
            }
          ]
        },
        "Policies": [
          {
            "PolicyName": {
              "Fn::Join": [
                "-",
                [
                  "flights-app",
                  "dev",
                  "lambda"
                ]
              ]
            },
            "PolicyDocument": {
              "Version": "2012-10-17",
              "Statement": [
                {
                  "Effect": "Allow",
                  "Action": [
                    "logs:CreateLogStream",
                    "logs:CreateLogGroup",
                    "logs:TagResource"
                  ],
                  "Resource": [
                    {
                      "Fn::Sub": "arn:${AWS::Partition}:logs:${AWS::Region}:${AWS::AccountId}:log-group:/aws/lambda/flights-app-dev*:*"
                    }
                  ]
                },
                {
                  "Effect": "Allow",
                  "Action": [
                    "logs:PutLogEvents"
                  ],
                  "Resource": [
                    {
                      "Fn::Sub": "arn:${AWS::Partition}:logs:${AWS::Region}:${AWS::AccountId}:log-group:/aws/lambda/flights-app-dev*:*:*"
                    }
                  ]
                },
                {
                  "Effect": "Allow",
                  "Action": [
                    "s3:*"
                  ],
                  "Resource": "*"
                }
              ]
            }
          }
        ],
        "Path": "/",
        "RoleName": {
          "Fn::Join": [
            "-",
            [
              "flights-app",
              "dev",
              {
                "Ref": "AWS::Region"
              },
              "lambdaRole"
            ]
          ]
        }
      }
    },
    "TransformUnderscoreloadLambdaFunction": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Code": {
          "ImageUri": "086729879076.dkr.ecr.us-east-1.amazonaws.com/serverless-flights-app-dev@sha256:8de7f89605ceb4de49e76cc044e0b2f77d3a442d38bbb39f7140bd60e62a67fa"
        },
        "PackageType": "Image",
        "FunctionName": "flights-app-dev-transform_load",
        "MemorySize": 10240,
        "Timeout": 45,
        "Architectures": [
          "arm64"
        ],
        "Role": {
          "Fn::GetAtt": [
            "IamRoleLambdaExecution",
            "Arn"
          ]
        }
      },
      "DependsOn": [
        "TransformUnderscoreloadLogGroup"
      ]
    },
    "TransformUnderscoreloadLambdaVersionEmhYbbj5lMnIYJeJsk39SbS7xQxV6GRiLq0NMBw": {
      "Type": "AWS::Lambda::Version",
      "DeletionPolicy": "Retain",
      "Properties": {
        "FunctionName": {
          "Ref": "TransformUnderscoreloadLambdaFunction"
        },
        "CodeSha256": "8de7f89605ceb4de49e76cc044e0b2f77d3a442d38bbb39f7140bd60e62a67fa"
      }
    },
    "ExtractUnderscoreloadLambdaFunction": {
      "Type": "AWS::Lambda::Function",
      "Properties": {
        "Code": {
          "ImageUri": "086729879076.dkr.ecr.us-east-1.amazonaws.com/serverless-flights-app-dev@sha256:85a9a069e548f1c8cd98a706c46f64e21b97bf0b067324606127fb126ff7406c"
        },
        "PackageType": "Image",
        "FunctionName": "flights-app-dev-extract_load",
        "MemorySize": 10240,
        "Timeout": 45,
        "Architectures": [
          "arm64"
        ],
        "Role": {
          "Fn::GetAtt": [
            "IamRoleLambdaExecution",
            "Arn"
          ]
        }
      },
      "DependsOn": [
        "ExtractUnderscoreloadLogGroup"
      ]
    },
    "ExtractUnderscoreloadLambdaVersionpFvaltdip0yejpdhZuJ0S6cZ6DTsXQWPjaHLl7T30": {
      "Type": "AWS::Lambda::Version",
      "DeletionPolicy": "Retain",
      "Properties": {
        "FunctionName": {
          "Ref": "ExtractUnderscoreloadLambdaFunction"
        },
        "CodeSha256": "85a9a069e548f1c8cd98a706c46f64e21b97bf0b067324606127fb126ff7406c"
      }
    },
    "MyDatabase": {
      "Type": "AWS::RDS::DBInstance",
      "Properties": {
        "AllocatedStorage": 20,
        "DBInstanceIdentifier": "flights-db",
        "Engine": "postgres",
        "EngineVersion": 14.7,
        "Port": 5432,
        "MasterUsername": "jigsaw",
        "MasterUserPassword": "jigsawlabs",
        "DBInstanceClass": "db.t3.micro",
        "PubliclyAccessible": true,
        "Parameters": {
          "DatabaseName": "flightsdb"
        }
      }
    }
  },
  "Outputs": {
    "ServerlessDeploymentBucketName": {
      "Value": {
        "Ref": "ServerlessDeploymentBucket"
      },
      "Export": {
        "Name": "sls-flights-app-dev-ServerlessDeploymentBucketName"
      }
    },
    "TransformUnderscoreloadLambdaFunctionQualifiedArn": {
      "Description": "Current Lambda function version",
      "Value": {
        "Ref": "TransformUnderscoreloadLambdaVersionEmhYbbj5lMnIYJeJsk39SbS7xQxV6GRiLq0NMBw"
      },
      "Export": {
        "Name": "sls-flights-app-dev-TransformUnderscoreloadLambdaFunctionQualifiedArn"
      }
    },
    "ExtractUnderscoreloadLambdaFunctionQualifiedArn": {
      "Description": "Current Lambda function version",
      "Value": {
        "Ref": "ExtractUnderscoreloadLambdaVersionpFvaltdip0yejpdhZuJ0S6cZ6DTsXQWPjaHLl7T30"
      },
      "Export": {
        "Name": "sls-flights-app-dev-ExtractUnderscoreloadLambdaFunctionQualifiedArn"
      }
    }
  }
}